{
	"publisher": "RenDan",
	"name": "geojson-io-for-vscode",
	"displayName": "geojson.io for vscode",
	"description": "",
	"version": "0.1.0",
	"engines": {
		"vscode": "^1.63.0"
	},
	"repository": "https://github.com/rend42/geojson.io-for-vscode.git",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:geojson-io-for-vscode.helloWorld",
		"onCommand:geojson-io-for-vscode.openWebview"
	],
	"main": "./out/src/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "geojson-io-for-vscode.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "geojson-io-for-vscode.openWebview",
				"title": "Open In Map"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "editorFocus && resourceExtname == .json",
					"command": "geojson-io-for-vscode.openWebview",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "npm-run-all compile:*",
		"compile:extension": "tsc -p ./",
		"compile:view": "webpack --mode development",
		"watch": "npm-run-all -p watch:*",
		"watch:extension": "tsc -watch -p ./",
		"watch:view": "webpack --watch --mode development",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/leaflet": "^1.7.9",
		"@types/leaflet-draw": "^1.0.5",
		"@types/mocha": "^9.0.0",
		"@types/node": "^17.0.18",
		"@types/react": "^17.0.38",
		"@types/react-dom": "^17.0.11",
		"@types/vscode": "^1.63.0",
		"@typescript-eslint/eslint-plugin": "^5.9.1",
		"@typescript-eslint/parser": "^5.9.1",
		"@vscode/test-electron": "^2.0.3",
		"css-loader": "^6.5.1",
		"eslint": "^8.6.0",
		"glob": "^7.2.0",
		"mocha": "^9.1.3",
		"npm-run-all": "^4.1.5",
		"style-loader": "^3.3.1",
		"ts-loader": "^9.2.6",
		"typescript": "^4.5.4",
		"webpack": "^5.66.0",
		"webpack-cli": "^4.9.1"
	},
	"dependencies": {
		"@turf/bbox": "^6.5.0",
		"leaflet": "^1.7.1",
		"leaflet-draw": "^1.0.4",
		"react": "^17.0.2",
		"react-dom": "^17.0.2"
	}
}
