{
  "publisher": "swallow",
  "name": "geojson-io-for-vscode",
  "displayName": "GeoJSON.IO for VSCode",
  "description": "Create, Edit and Preview GeoJSON Data In VSCode",
  "version": "0.3.8",
  "engines": {
    "vscode": "^1.63.0"
  },
  "icon": "images/icon.png",
  "repository": "https://github.com/rend42/geojson.io-for-vscode.git",
  "categories": [
    "Other",
    "Data Science",
    "Visualization"
  ],
  "activationEvents": [
    "onCommand:geojson-io-for-vscode.openMap"
  ],
  "main": "./out/src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "geojson-io-for-vscode.openMap",
        "title": "View On Map"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorFocus && resourceExtname== '.json'",
          "command": "geojson-io-for-vscode.openMap",
          "group": "navigation"
        },
        {
          "when": "editorFocus && resourceExtname== '.txt'",
          "command": "geojson-io-for-vscode.openMap",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "yarn compile:extension && yarn compile:view",
    "compile:extension": "tsc -p ./",
    "compile:view": "webpack --mode production",
    "watch": "npm-run-all -p watch:*",
    "watch:extension": "tsc -watch -p ./",
    "watch:view": "webpack --watch --mode development",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "release": "standard-version"
  },
  "devDependencies": {
    "@types/esri-leaflet": "^2.1.9",
    "@types/geojson": "^7946.0.8",
    "@types/glob": "^7.2.0",
    "@types/leaflet": "^1.7.9",
    "@types/leaflet-draw": "^1.0.5",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.18",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/vscode": "^1.63.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vscode/test-electron": "^2.0.3",
    "commitizen": "^4.2.4",
    "css-loader": "^6.5.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.6.0",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^12.6.0",
    "standard-version": "^9.3.2",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "@turf/area": "^6.5.0",
    "@turf/bbox": "^6.5.0",
    "@turf/center": "^6.5.0",
    "@turf/helpers": "^6.5.0",
    "@turf/length": "^6.5.0",
    "esri-leaflet": "^3.0.8",
    "esri-leaflet-vector": "^3.1.2",
    "leaflet": "^1.7.1",
    "leaflet-draw": "^1.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
